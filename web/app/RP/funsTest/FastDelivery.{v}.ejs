<div class="divcontent">
    <div id="handout_wrap_inner"></div>
    <h1>查快递单号</h1>
    <hr>
    <label class="layui-form-label">公司:</label>
    <div class="layui-inline">
        <input class="layui-input" name="Shipper" id="Shipper" value="ZTO" placeholder="输入快递公司代号:SF" autocomplete="off">
    </div>
    <br></br>
    <label class="layui-form-label">单号:</label>
    <div class="layui-inline">
        <input class="layui-input" name="Logistic" id="Logistic" value="73125715402431" placeholder="输入单号:2100123465"
            autocomplete="off">
    </div>

    <div class="lineitem">
        <button id="fastnoteBtn" class="btn btn-info">查讯快递单号</button>
    </div>
    <br />
    <div id="div_result" name="div_result" class="label-basic">基本
    </div>
    <br />
    <div id="div_track" name="div_track" class="label-track">轨迹
    </div>
</div>
<script>
    //页面加载函数---------------
    $(document).ready(function () {
        layui.use(['form', 'layedit', 'laydate'], function () {
            var form = layui.form,
                layer = layui.layer,
                layedit = layui.layedit,
                laydate = layui.laydate;

            form.render();
        });

        
        //判断是否选中数据-------
        $('#fastnoteBtn').click(async function () {

            var company = $("#Shipper").val();
            var expressId = $("#Logistic").val();

            let r = await getExpressTrack(company, expressId)
            alert(r);

        });


    });
</script>